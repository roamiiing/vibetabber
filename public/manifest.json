{
    "manifest_version": 3,
    "name": "VibeTabber",
    "version": "1.0.0",
    "description": "Extension for managing tabs like Arc",
    "permissions": ["tabs", "sidePanel", "storage"],
    "side_panel": {
        "default_path": "index.html"
    },
    "action": {
        "default_title": "Open Tabs"
    },
    "background": {
        "service_worker": "sw.js"
    }
}
